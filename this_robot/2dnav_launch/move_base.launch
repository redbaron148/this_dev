<launch>
  <master auto="start"/>
  
  <!--include file="$(find this_robot)/gmapping_launch/this_gmapping.launch"/-->
  
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find this_robot)/2dnav_launch/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find this_robot)/2dnav_launch/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find this_robot)/2dnav_launch/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find this_robot)/2dnav_launch/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find this_robot)/2dnav_launch/base_local_planner_params.yaml" command="load" />
  </node>
</launch>
