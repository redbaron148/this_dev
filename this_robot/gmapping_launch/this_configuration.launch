<launch>
	<include file="$(find this_robot)/launch/scan_filter.launch"/>

  	<node pkg="irobot_create_2_1" type="driver.py" name="create_driver" output="screen">
		<param name="port" value="/dev/ttyUSB0" />
	</node>
 	
 	<node pkg="tf" type="static_transform_publisher" name="laser_to_base_link" args="0.1651 0 0.03175 0 0 3.14159 base_link laser 10"/>
 	<!--node pkg="tf" type="static_transform_publisher" name="base_link_to_odom" args="0 0 0 0 0 0 base_link odom 10"/-->
</launch>
